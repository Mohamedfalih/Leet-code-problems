int thirdMax(int* nums, int numsSize) {
    
    for(int i=0; i<numsSize; i++)
	{
		for(int j=i+1; j<numsSize; j++)
		{
			if(nums[i]<nums[j])
			{
				int temp=nums[i];
				nums[i]=nums[j];
				nums[j]=temp;
			}
		}
	}
    int flag = 1;
    for (int i = 1; i < numsSize; i++) {
        if (nums[i] != nums[i - 1]) {
            flag++;
        }
        if (flag == 3) {
            return nums[i]; 
        }
    }
    return nums[0];
}
