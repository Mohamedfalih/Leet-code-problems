bool isValid(char* s) {
    int len = strlen(s);
    char* stack = (char*)malloc(sizeof(char) * len); 
    int top = -1;

    for (int i = 0; i < len; i++) {
        char c = s[i];

        if (c == '(' || c == '{' || c == '[') {
            stack[++top] = c;
        } else if (c == ')') {
            if (top < 0 || stack[top] != '(') {
                free(stack);
                return false;
            }
            top--;
        } else if (c == '}') {
            if (top < 0 || stack[top] != '{') {
                free(stack);
                return false;
            }
            top--;
        } else if (c == ']') {
            if (top < 0 || stack[top] != '[') {
                free(stack);
                return false;
            }
            top--;
        }
    }

    bool result = (top == -1);
    free(stack);
    return result;
}
